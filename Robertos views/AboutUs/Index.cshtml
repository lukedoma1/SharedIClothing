@model IEnumerable<Group8_iCLOTHINGApp.Models.AboutUs>

@{
    ViewBag.Title = "Index";
}

<h2 class="text-center">About us</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.companyAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.companyShippingPolicy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.companyReturnPolicy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.companyContactInfo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.companyBusinessDescription)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.companyAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.companyShippingPolicy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.companyReturnPolicy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.companyContactInfo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.companyBusinessDescription)
        </td>
    </tr>
}

</table>
